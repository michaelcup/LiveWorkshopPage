# Keap API Configuration
# Copy this file to .env for local development (DO NOT commit .env to Git!)
# For Netlify: Set these in Site Settings > Environment Variables

# Your Keap Personal Access Token
# Get from: Keap > Settings > Application > API > Personal Access Tokens
KEAP_ACCESS_TOKEN=your_token_here

# Custom Field IDs (get from Keap custom field URLs)
# Example: https://abc123.infusionsoft.com/admin/customFields/25 -> ID is 25
KEAP_ROLE_FIELD_ID=25
KEAP_QUESTIONS_FIELD_ID=27
KEAP_INTEREST_FIELD_ID=29
KEAP_CHALLENGES_FIELD_ID=31
KEAP_PREFERRED_CONTACT_FIELD_ID=33

# Tag IDs (get from Keap tag URLs)
# Example: https://abc123.infusionsoft.com/Contact/manageTagsRibbon.jsp?view=edit&id=123 -> ID is 123
KEAP_WORKSHOP_TAG_ID=123
KEAP_CORPORATE_TAG_ID=456

# Optional: SendGrid API Key for email notifications
# Get from: https://app.sendgrid.com/settings/api_keys
# SENDGRID_API_KEY=your_sendgrid_key_here
