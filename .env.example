# Keap API Configuration - Workshop Registration Page
# Set these in Netlify: Site Settings > Environment Variables

# Your Keap Personal Access Token
# Get from: Keap > Settings > Application > API > Personal Access Tokens
KEAP_ACCESS_TOKEN=your_token_here

# Custom Field IDs for workshop form
# Click on each field in Keap and get the ID from the URL
KEAP_ROLE_FIELD_ID=297
KEAP_QUESTIONS_FIELD_ID=299

# Tag ID for workshop registrations
KEAP_WORKSHOP_TAG_ID=1717
